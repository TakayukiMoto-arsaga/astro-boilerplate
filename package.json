{
  "name": "astro-boilerplate",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prepare": "panda codegen --clean",
    "dev": "astro dev --host",
    "start": "astro dev",
    "build": "astro check && astro build",
    "astro:check": "astro check",
    "preview": "astro preview",
    "astro": "astro",
    "studio": "panda studio",
    "check-deps": "ncu",
    "update-deps": "ncu -u",
    "check": "biome check ./src",
    "lint:eslint": "eslint ./src",
    "lint:markup": "markuplint \"src/**/*.{astro,jsx,tsx}\" --problem-only",
    "lint:text": "textlint \"**/*.{md,mdx}\" \"!**/node_modules\"",
    "lint:markdown": "markdownlint-cli2 \"**/*.{md,mdx}\" \"!**/node_modules\""
  },
  "dependencies": {
    "@ark-ui/react": "^3.12.0",
    "@astrojs/check": "^0.9.3",
    "@astrojs/mdx": "^4.0.0-beta.1",
    "@astrojs/react": "^3.6.2",
    "@astrojs/sitemap": "^3.1.6",
    "@textlint/textlint-plugin-markdown": "^14.2.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "astro": "^5.0.0-beta.1",
    "lucide-react": "^0.441.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.78.0",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.0",
