    "@markuplint/astro-parser": "^4.6.6",
    "@markuplint/jsx-parser": "^4.7.6",
    "@markuplint/react-spec": "^4.5.6",
    "@pandacss/dev": "^0.46.0",
    "@pandacss/eslint-plugin": "^0.1.10",
    "@pandacss/studio": "^0.45.0",
    "@park-ui/cli": "^0.9.0",
    "@park-ui/panda-preset": "^0.42.0",
    "@textlint-ja/textlint-rule-no-insert-dropping-sa": "^2.0.1",
    "@textlint-ja/textlint-rule-no-synonyms": "^1.3.0",
    "@typescript-eslint/parser": "^8.5.0",
    "astro-eslint-parser": "^1.0.3",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "lefthook": "^1.7.15",
